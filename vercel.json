{
  "version": 2,
  "builds": [{
    "src": "server.js",
    "use": "@vercel/node"
  }],
  "routes": [
    {
      "src": "/keys",
      "dest": "server.js",
      "methods": ["GET"]
    },
    {
      "src": "/unbind",
      "dest": "server.js",
      "methods": ["POST"]
    },
    {
      "src": "/bind",
      "dest": "server.js",
      "methods": ["POST"]
    }
  ],
  "env": {
    "MONGODB_URI": "@mongodb_uri"
  }
}
